const S = 2000000
const p = 10
const лет = 5
функция Perepl (S,p,years) {
  const n = годы * 12
  const i = (p / 100) / 12
  const everyMonth = S * ((i*(1+i)**n)/((1+i)* *n - 1))
  const perepl = Math.round(n * everyMonth - S) 
  возвращение перепл
}

const perepl = Perepl(S,p,годы)
консоль.журнал("№8 ||||", перепл, "|||/|")
