//7
const nFib = 12
функция fib(nFib) {
  пусть a = 1;
  пусть b = 1;
  для (Пусть i = 3; i <= nFib; i++) {
    пусть c = a + b;
    a = b;
    b = c;
  }
  возвращение b;
}
  const el12 = Math.round(fib(nFib))
  консоль.журнал("№7 ||||", el12, "|||/|")

